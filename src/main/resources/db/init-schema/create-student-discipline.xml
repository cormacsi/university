<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.1.xsd">

    <changeSet id="CREATE_STUDENT_DISCIPLINE_TABLE" author="cormacsi">
        <preConditions onFail="MARK_RAN">
            <not>
                <tableExists tableName="student_discipline"/>
            </not>
        </preConditions>
        <createTable tableName="student_discipline">
            <column name="student_id" type="BIGINT">
                <constraints foreignKeyName="fk_student_discipline_student_id" references="student(id)" nullable="false"/>
            </column>
            <column name="discipline_id" type="BIGINT">
                <constraints foreignKeyName="fk_student_discipline_discipline_id" references="discipline(id)" nullable="false"/>
            </column>
        </createTable>
    </changeSet>

</databaseChangeLog>